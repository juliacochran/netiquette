{
  "manifest_version": 2,

  "name": "Netiquette",
  "description": "This extension provides tips about proper online etiquette.",
  "version": "1.0",

  "icons": { "48": "images/logo48.png",
          "128": "images/logo128.png" },

//not working
 "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "run_at": "document_end",
      "js": ["javascripts/contentscript.js",
             "javascripts/jquery-ui.min,js",
              "javascripts/jquery.min.js"

            ],

        "css": [ "jquery-ui.css" ]
    }
  ],
 "permissions": ["tabs", "http://*/*", "background"],

 "background": "background.html",

  //must list all css files here
  "web_accessible_resources": [
    "styles/popup.css"
  ],

  "browser_action": {
    "default_icon": "images/logo19.png",
    "default_popup": "popup.html"
  }
}